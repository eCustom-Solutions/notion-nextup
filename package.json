{
  "name": "notion-nextup",
  "version": "1.0.0",
  "description": "CLI tool to process Notion databases and create ranked task queues",
  "main": "src/cli/notion-nextup.ts",
  "scripts": {
    "start": "ts-node src/cli/notion-nextup.ts",
    "build": "tsc",
    "dev": "ts-node --watch src/cli/notion-nextup.ts",
    "dev:notion": "ts-node src/cli/notion-nextup.ts",
    "setup": "./scripts/setup-env.sh",
    "debug": "ts-node src/utils/debug-tasks.ts",
    "perf": "ts-node src/utils/performance-test.ts",
    "test:notion": "ts-node src/tests/notion-integration.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "dotenv": "^17.2.1"
  }
}
