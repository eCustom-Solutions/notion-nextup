{
  "name": "notion-nextup",
  "version": "1.0.0",
  "description": "High-performance CLI tool for Notion task queue optimization with surgical updates, database-level filtering, and 85% performance improvement",
  "keywords": ["notion", "task-queue", "productivity", "cli", "typescript", "optimization"],
  "author": "theHaruspex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theHaruspex/notion-nextup.git"
  },
  "main": "src/cli/notion-nextup.ts",
  "scripts": {
    "start": "ts-node src/cli/notion-nextup.ts",
    "build": "tsc",
    "dev": "ts-node --watch src/cli/notion-nextup.ts",
    "dev:notion": "ts-node src/cli/notion-nextup.ts",
    "setup": "./scripts/setup-env.sh",
    "debug": "ts-node src/utils/debug-tasks.ts",
    "perf": "ts-node src/utils/performance-test.ts",
    "test:notion": "ts-node src/tests/notion-integration.ts",
    "start:webhook": "ts-node src/webhook/http/prod-server.ts",
    "start:demo": "ts-node src/webhook/http/demo-server.ts",
    "estimated-hours:copy-demo": "ts-node scripts/estimated-hours/copy-demo-estimated-days.ts",
    "estimated-hours:convert-days-to-hours": "ts-node scripts/estimated-hours/convert-days-to-hours.ts",
    "estimated-hours:convert-staging-in-place": "ts-node scripts/estimated-hours/convert-staging-in-place.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2"
  }
}
